<h1>Coordinators</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <% @coordinators.each do |coordinator| %>
      <tr>
        <td><%= coordinator.name %></td>
        <td><%= coordinator.email %></td>
        <td><%= coordinator.phone_number %></td>
        <td><%= link_to 'Show', coordinator %></td>
        <td><%= link_to 'Edit', edit_coordinator_path(coordinator) %></td>
        <td><%= link_to 'Export CSV', export_participants_coordinator_path(coordinator, format: :csv) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Coordinator', new_coordinator_path %>
